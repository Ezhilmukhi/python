def most_frequency(string):
    dict={}
    for i in string:
        keys=dict.keys()
        if i in keys:
            dict[i]+=1
        else:
            dict[i]=1
    for i in dict:
        print(i,'=',dict[i])
a=input("Enter the string:")
print(most_frequency(a))


OUTPUT:
Enter the string:education
e = 1
d = 1
u = 1
c = 1
a = 1
t = 1
i = 1
o = 1
n = 1
None
